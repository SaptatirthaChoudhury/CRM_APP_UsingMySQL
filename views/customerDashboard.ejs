<%- include("partials/customerDashboardPage/header") -%>

    <body id="customerDashboard" class="card-body">
        <div class="containerCustomer">
            <div class="profile">
                <h1 id="h1" class="card-title display-4">Welcome, <%= complains[0].customer_Name %>
                </h1>
                <p id="p" class="card-text text-muted">Email: <%= complains[0].customer_Email %>
                </p>

            </div>
            <div class="complaint-history">
                <h2>Complaint History</h2>
                <div class="table-wrapper">
                    <table class="table table-striped">
                        <thead>
                            <tr>

                                <th scope="col">Complaint Category</th>
                                <th scope="col">Complaint ID</th>
                                <th scope="col">status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for (let i=0; i < complains.length; i++) { %>
                                <tr>
                                    <td>
                                        <%= complains[i].complain_Category %>
                                    </td>
                                    <td>
                                        <%= complains[i].id %>
                                    </td>
                                    <td>
                                        <%= complains[i].status %>
                                    </td>
                                </tr>
                                <% } %>

                                    <!-- Add more rows for additional complaint history entries -->
                        </tbody>
                    </table>
                </div>

            </div>

           
        </div>

        <script src="/public/Js/customerDashboard.js"></script>

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
            crossorigin="anonymous"></script>
    </body>

    <%- include("partials/customerDashboardPage/footer") -%>